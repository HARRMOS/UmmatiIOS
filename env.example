# Configuration Google OAuth (OBLIGATOIRE pour la connexion Google)
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# URLs de l'application
BACKEND_URL=https://appislamic.onrender.com
FRONTEND_URL=https://ummati.pro

# JWT Secret (générer une clé sécurisée)
JWT_SECRET=your-super-secret-jwt-key-here-change-this-in-production

# Configuration Base de données MySQL
DB_HOST=your-db-host
DB_PORT=3306
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_NAME=your-db-name

# Configuration Serveur
PORT=3000
NODE_ENV=production

# Admin Password (pour la connexion admin)
ADMIN_PASSWORD=your-admin-password-here

